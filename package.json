{
  "name": "vue-svg-primitives",
  "description": "A component Library of drawing svg for Vue.js",
  "keywords": [
    "vue",
    "svg",
    "primitive",
    "library"
  ],
  "version": "0.0.1",
  "license": "MIT",
  "repository": "github:resessh/vue-svg-primitives",
  "author": {
    "name": "Xuesu Li",
    "email": "info@resessh.net",
    "url": "http://resessh.net/"
  },
  "main": "dist/vue-svg-primitives.umd.js",
  "module": "dist/vue-svg-primitives.esm.js",
  "unpkg": "dist/vue-svg-primitives.min.js",
  "files": [
    "dist/*",
    "src/*",
    "!src/lib-dev.vue"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:umd": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format umd",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {},
  "devDependencies": {
    "cross-env": "^5.2.0",
    "minimist": "^1.2.0",
    "rollup": "^1.4.1",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-vue": "^4.7.2",
    "vue": "^2.6.8",
    "vue-template-compiler": "^2.6.8"
  }
}
